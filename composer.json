{
    "name": "survos/command-bundle",
    "description": "Run Symfony console commands from a web interface",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "symfony/config": "^6.4 || ^7.1",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/form": "^6.4 || ^7.1",
        "symfony/console": "^6.4 || ^7.1",
        "zenstruck/console-extra": "^1.4",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/messenger": "^6.4 || ^7.1",
        "symfony/yaml": "^6.4 || ^7.1",
        "phpunit/phpunit": "^11.4",
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Survos\\CommandBundle\\": "src/",
            "Survos\\CommandBundle\\Tests\\": "tests/"
        }
    }
}
