{
    "name": "survos/command-bundle",
    "description": "Run Symfony console commands from a web interface",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.1.0",
        "symfony/dependency-injection": "^6.1.0",
        "symfony/http-kernel": "^6.1.0",
        "symfony/framework-bundle": "^6.1.0",
        "symfony/form": "^6.1.0",
        "symfony/console": "^6.1.0",
        "zenstruck/console-extra": "^1.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Survos\\CommandBundle\\": "src/",
            "Survos\\CommandBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.8"
    }
}
